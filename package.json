{
  "name": "wheremymobi",
  "description": "Find mobi bikes in Vancouver, BC.",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Pat Sissons",
    "email": "patricksissons@gmail.com",
    "url": "https://github.com/patsissons"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patsissons/wheremymobi"
  },
  "bugs": {
    "url": "https://github.com/patsissons/wheremymobi/issues"
  },
  "homepage": "https://patsissons.github.io/wheremymobi/",
  "keywords": [
    "mobi",
    "bike"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn gatsby build",
    "clean": "del docs .cache",
    "deploy": "yarn clean && yarn build --prefix-paths && ncp public docs",
    "dev": "yarn gatsby develop",
    "gatsby": "ts-node node_modules/gatsby-cli/lib/index.js",
    "lint": "yarn lint:ts",
    "lint:stats": "yarn lint:ts --format node_modules/eslint-stats/byError.js",
    "lint:ts": "eslint . --ext .ts --ext .tsx --max-warnings 0 --format codeframe",
    "pretty": "prettier --write '**/*.{css,scss,js,json,md,ts,tsx,yml}'",
    "server": "yarn gatsby serve",
    "start": "yarn dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=11.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-sass": "^2.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "graphql": "^14.0.0",
    "moment": "^2.0.0",
    "node-sass": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-google-maps": "^9.0.0",
    "react-spinkit": "^3.0.0",
    "recompose": "^0.30.0",
    "rxjs": "^6.0.0",
    "typescript": "~3.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.0.0",
    "@types/fs-extra": "^5.0.0",
    "@types/googlemaps": "^3.0.0",
    "@types/graphql": "^14.0.0",
    "@types/node": "^11.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-spinkit": "^3.0.0",
    "@types/recompose": "^0.30.0",
    "del-cli": "^1.0.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.0.0",
    "eslint-plugin-shopify": "^26.0.0",
    "eslint-stats": "^1.0.0",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.0",
    "ncp": "^2.0.0",
    "prettier": "^1.0.0",
    "pretty-quick": "^1.0.0",
    "sass-loader": "^7.0.0",
    "ts-node": "^8.0.0",
    "tsconfig-paths-webpack-plugin": "^3.0.0"
  },
  "resolutions": {
    "graphql": "14.1.1",
    "typescript-eslint-parser": "22.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:shopify/react",
      "plugin:shopify/typescript",
      "plugin:shopify/jest",
      "plugin:shopify/prettier"
    ],
    "settings": {
      "react": {
        "version": "16.8"
      }
    },
    "rules": {
      "import/no-anonymous-default-export": "off",
      "import/no-named-as-default": "off",
      "no-empty-pattern": "off",
      "shopify/images-no-direct-imports": "off",
      "babel/object-curly-spacing": "off",
      "prettier/prettier": "off",
      "import/named": "off",
      "shopify/strict-component-boundaries": "off"
    },
    "overrides": []
  },
  "eslintIgnore": [
    "/.cache",
    "/build",
    "/docs",
    "/public"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 chromeandroid versions",
    "last 3 firefox versions",
    "last 3 opera versions",
    "last 2 edge versions",
    "safari >= 10",
    "ios >= 9"
  ]
}
