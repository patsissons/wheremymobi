{
  "name": "wheremymobi",
  "description": "Find mobi bikes in Vancouver, BC.",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^22",
    "pnpm": "^10"
  },
  "packageManager": "pnpm@10.14.0",
  "author": {
    "name": "Pat Sissons",
    "email": "patricksissons@gmail.com",
    "url": "https://github.com/patsissons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patsissons/wheremymobi.git"
  },
  "bugs": {
    "url": "https://github.com/patsissons/wheremymobi/issues"
  },
  "homepage": "https://wheremymobi.com",
  "keywords": [
    "mobi",
    "bike"
  ],
  "license": "MIT",
  "scripts": {
    "conf": "pnpm run conf:link && pnpm run conf:env",
    "conf:link": "test -f .vercel/project.json || vercel link",
    "conf:env": "./scripts/setup-env.sh",
    "conf:clean": "rm -rf .vercel .env*.local",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:prettier": "prettier --check .",
    "lint:es": "eslint .",
    "lint": "pnpm lint:prettier && pnpm lint:es",
    "format": "prettier --write .",
    "validate": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.31.1",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.21",
    "dayjs": "^1.11.13",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "lodash": "^4.17.21",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "svelte-collapse": "^0.1.4",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.20.0",
    "vercel": "^46.0.1",
    "vite": "^7.1.2"
  }
}
